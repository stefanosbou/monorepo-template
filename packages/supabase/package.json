{
  "name": "@humblebrag/supabase",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "biome check .",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "db:generate": "supabase gen types --lang=typescript --project-id $PROJECT_ID --schema public > src/types/db.ts"
  },
  "dependencies": {
    "@supabase/postgrest-js": "^2.95.3",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.95.3",
    "date-fns": "^4.1.0",
    "react": "catalog:react",
    "server-only": "^0.0.1",
    "supabase": "^2.76.5",
    "next": "catalog:next"
  },
  "devDependencies": {
    "typescript": "catalog:typescript",
    "@humblebrag/tsconfig": "workspace:*",
    "@types/node": "catalog:types",
    "@types/react": "catalog:types"
  },
  "exports": {
    "./server": "./src/client/server.ts",
    "./client": "./src/client/client.ts",
    "./client-query": "./src/queries/client.ts",
    "./mutations": "./src/mutations/index.ts",
    "./middleware": "./src/client/middleware.ts",
    "./queries": "./src/queries/index.ts",
    "./cached-queries": "./src/queries/cached-queries.ts",
    "./storage": "./src/utils/storage.ts",
    "./types": "./src/types/index.ts"
  }
}
